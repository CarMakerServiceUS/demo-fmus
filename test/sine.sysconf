; A sine oscillation generated by a spring-mass system, together with a
; mathematical sine function for comparison.
slaves {
    spring {
        type spring_1d
    }
    mass {
        type mass_1d
    }
    sine {
        type sfh.larky.sine
        init {
            a 1.0
            d -1.57079632679
        }
    }
}
connections {
    mass.force1 spring.force_b
    spring.pos_b mass.pos
}
